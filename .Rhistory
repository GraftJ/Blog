library(TurtleGraphics)
turtle_star <- function(ray = 6, len = 20, color = "red", type = 1, width = 1){
for (i in 1:ray){
turtle_up()
turtle_backward(len)
turtle_down()
turtle_col(color)
turtle_lty(type)
turtle_lwd(width)
turtle_forward(2*len)
turtle_up()
turtle_backward(len)
turtle_down()
turtle_left(180/ray)
}
turtle_hide()
}
turtle_init(mode = "clip")
turtle_star(ray = 10, len = 20)
turtle_init(mode = "clip")
turtle_star(ray = 10, len = 20, type = 2)
